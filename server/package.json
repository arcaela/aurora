{
    "version": "1.0.33",
    "main": "./dist/index.js",
    "name": "@arcaela/server",
    "description": "Toolkit for building backend applications using the front-end.",
    "scripts": {
        "patch": "npm version patch",
        "@publish": "npm publish --access=public",
        "@deploy": "npm run patch && npm run @publish",
        "prepare": "set NODE_ENV=production && npx babel ./src/ --out-dir ./dist/ --minified"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arcaela/aurora.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "*"
    ],
    "engines": {
        "node": ">=10"
    },
    "keywords": [
        "serverless",
        "aurora",
        "arcaela",
        "javascript"
    ],
    "author": "arcaela.reyes@gmail.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arcaela/aurora/issues"
    },
    "homepage": "https://github.com/arcaela/aurora#readme",
    "dependencies": {
        "lodash": "latest",
        "path-to-regex": "^1.3.8"
    },
    "devDependencies": {
